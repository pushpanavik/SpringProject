package com.bridgeit.springPractice;

import org.springframework.context.ApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;

public class EmpMain {

	public static void main(String args[])
	{
		ApplicationContext applicationContext=new ClassPathXmlApplicationContext("/springPractice/src/main/java/com/bridgeit/springPractice/Bean.xml");
		Employee e=(Employee) applicationContext.getBean("emp");
		e.show();
	}
}
